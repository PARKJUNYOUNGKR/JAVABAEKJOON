package univ94;
import java.util.*;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int h = sc.nextInt();
		int m = sc.nextInt();
		
		if(m>=45) {
			System.out.println(h + " " + (m-45));
		}
		else {
			if(h==0) {
				h=23;
				System.out.println(h+" "+((45+m)-30));
			}
			else {
				System.out.println((h-1)+ " " + ((45 + m)-30));
			}
		}
	}

}

//right problem
